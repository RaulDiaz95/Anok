{
  "name": "order-management",
  "version": "1.0.0",
  "description": "3D Figure Order Management System - Full Stack (Spring Boot + React)",
  "private": true,
  "scripts": {
    "dev:frontend": "cd Anoktest-web && npm run dev",
    "dev:backend": "cd Anoktest-api && ./mvnw spring-boot:run",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:Anoktest-api\"",
    "build:frontend": "cd Anoktest-web && npm run build",
    "build:backend": "cd Anoktest-api && ./mvnw clean package",
    "build": "npm run build:frontend && npm run build:Anoktest-api",
    "test:frontend": "cd Anoktest-web && npm run test",
    "test:backend": "cd Anoktest-api && ./mvnw test",
    "test": "npm run test:frontend && npm run test:Anoktest-api",
    "setup": "cd Anoktest-web && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}